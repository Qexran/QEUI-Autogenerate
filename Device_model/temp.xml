    <!--控制中心/系统界面-->
    <control_ui>
        <!--支持新版控制中心/默认使用新版控制中心-->
        <bool name="support_use_new_control">false</bool>
        <!--支持控制中心进出缩放动画-->
        <bool name="support_control_int_out_anim">false</bool>
        <!--支持音量条、通知动态模糊-->
        <bool name="support_volume_notification_blur">false</bool>
