        <bool name="sc_support_display_battery_temperature">false</bool>
        <!--支持模糊定位-->
        <bool name="sc_support_blur_locate">false</bool>
        <!--支持模糊定位-->
        <bool name="sc_support_blur_locate">false</bool>
    </security_center>
</device_properties>