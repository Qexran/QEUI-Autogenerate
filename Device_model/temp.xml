        <!--支持运动抓拍-->
        <bool name="support_movement_catch">false</bool>
        <!--支持去痣-->
        <bool name="support_remove_zhi">false</bool>
    </camera>

    <!--相册-->
    <gallery>
        <!--支持"推荐"栏目-->
        <bool name="support_recommend_colum">false</bool>
        <!--支持魔法换天-->
        <bool name="support_magic_change_cloud">false</bool>
        <!--支持文字烟花-->
