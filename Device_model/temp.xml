        <bool name="sc_support_clear_play">false</bool>
        <!--视频工具箱支持智能刷新率-->
        <bool name="sc_support_intell_rate">false</bool>
        <!--支持自动连招-->
        <bool name="sc_support_auto_operate">false</bool>
        <!--支持显示电池温度-->
        <bool name="sc_support_display_battery_temperature">false</bool>
