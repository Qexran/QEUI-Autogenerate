        <!--支持三项AI大师画质引擎-->
        <bool name="support_ai_video_quality">false</bool>
        <!--支持前置摄像美颜-->
        <bool name="support_beautify_camera">false</bool>
        <!--支持自定义屏幕刷新率(60fps的机型请输入true)-->
